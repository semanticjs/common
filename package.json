{
  "name": "@semanticjs/common",
  "version": "0.0.8",
  "description": "",
  "main": "./packages/cjs/index.js",
  "module": "./packages/es/index.js",
  "files": [
    "packages/"
  ],
  "scripts": {
    "build": "concurrently \"npm run build:es\" \"npm run build:cjs\"",
    "build:es": "tsc -p tsconfig.es.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "deploy": "npm version patch && npm run deploy:all",
    "deploy:all": "npm run build && npm publish --access public",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "watch": "npm run watch:es",
    "watch:es": "tsc -p tsconfig.es.json -w",
    "watch:cjs": "tsc -p tsconfig.cjs.json -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semanticjs/common.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/semanticjs/common/issues"
  },
  "homepage": "https://github.com/semanticjs/common#readme",
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.24",
    "concurrently": "^6.2.0",
    "jest": "^27.0.6",
    "node-fetch": "^2.6.1",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  }
}
