{
  "name": "@semanticjs/common",
  "version": "0.0.17",
  "description": "",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "typings": "./dist/src/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && npm run rollup",
    "build:tsc": "concurrently \"npm run build:esm\" \"npm run build:cjs\"",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "clean": "rimraf dist",
    "deploy": "npm version patch && npm run deploy:all",
    "deploy:all": "npm run build && npm publish --access public",
    "rollup": "rollup -c",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "watch": "npm run watch:esm",
    "watch:esm": "tsc -p tsconfig.esm.json -w",
    "watch:cjs": "tsc -p tsconfig.cjs.json -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semanticjs/common.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/semanticjs/common/issues"
  },
  "homepage": "https://github.com/semanticjs/common#readme",
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.9.2",
    "concurrently": "^6.2.0",
    "jest": "^27.0.6",
    "node-fetch": "^2.6.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  }
}
